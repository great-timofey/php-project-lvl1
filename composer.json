{
    "name": "timmy/brain-games",
    "authors": [
        {
            "name": "Timofei Tverdokhlebov",
            "email": "great.timofey@icloud.com"
        }
    ],
    "description": "Set of console games",
    "type": "library",
    "keywords": [
        "game",
        "console",
        "fun"
    ],
    "bin": [
        "bin/brain-even",
        "bin/brain-calc",
        "bin/brain-gcd",
        "bin/brain-progression",
        "bin/brain-prime"
    ],
    "autoload": {
        "files": [
            "src/game-engine.php",
            "src/games/even.php",
            "src/games/calc.php",
            "src/games/gcd.php",
            "src/games/progression.php",
            "src/games/prime.php"
        ]
    },
    "require": {
        "wp-cli/php-cli-tools": "^0.11.11"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4"
    },
    "scripts": {
        "phpcs": "./vendor/bin/phpcs",
        "phpcbf": "./vendor/bin/phpcbf"
    }
}
